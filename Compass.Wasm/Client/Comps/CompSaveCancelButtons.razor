@inject NavigationManager NavigationManager
<div class="mt-3">
    <button class="btn btn-success me-2" type="submit">保存</button>
    <button class="btn btn-secondary" type="button" @onclick="@(() => NavigationManager.NavigateTo(CancelUrl))">取消</button>
</div>

@code {
    [Parameter]
    public string CancelUrl { get; set; }
}
